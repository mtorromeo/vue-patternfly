{
  "name": "@vue-patternfly/table",
  "version": "5.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch:*'",
    "watch:vite": "vite build --watch",
    "watch:tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@patternfly/react-styles": "^5.4.1",
    "@vue-patternfly/core": "workspace:^",
    "@vue-patternfly/icons": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^13.0.0",
    "glob": "^11.0.1",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "@vue-patternfly/core": "workspace:^",
    "@vueuse/core": "^13"
  }
}
