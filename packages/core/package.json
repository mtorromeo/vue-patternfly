{
  "name": "@vue-patternfly/core",
  "version": "5.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly && node scripts/build-global.mjs",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch:*'",
    "watch:vite": "vite build --watch",
    "watch:tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "watch:main.d.ts": "nodemon --watch dist/main.d.ts --watch 'dist/**/index.d.ts' --watch scripts/build-global.mjs -x \"node scripts/build-global.mjs\"",
    "prepack": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@floating-ui/core": "^1.7.3",
    "@floating-ui/dom": "^1.7.4",
    "@patternfly/patternfly": "^5.4.2",
    "@patternfly/react-styles": "^5.4.1",
    "@patternfly/react-tokens": "^5.4.1",
    "@vue-patternfly/icons": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/web-bluetooth": "^0.0.21",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/compiler-sfc": "^3.5.25",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "@vueuse/shared": "^14.0.0",
    "concurrently": "^9.2.1",
    "focus-trap": "^7.6.6",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.5"
  },
  "peerDependencies": {
    "@vueuse/core": "^13",
    "@vueuse/integrations": "^13",
    "@vueuse/shared": "^13",
    "focus-trap": "*"
  }
}
